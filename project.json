   "logicalFrame": {
       "goal": "To contribute to the reduction of neonatal mortality and maternal mortality in Afar region of Ethiopia by the next Ethiopia Demographic and Health Survey (EDHS)",
       "purposes": [
           {
               "description": "To reduce the prevalence and the harmful consequences of female genital mutilation in Zone 5 of Afar region by 2015",
               "assumptions": "There is low risk for lies or false declarations during surveys. There is low to medium risk for the practice of female genital mutilation to continue through less severe forms, in secret and/or in neighbouring areas. The Afar region is homogeneous so that baseline values calculated for the whole of the region (EDHS 2005) are applicable to the sole Zone 5",
               "outputs": [
                   {
                       "description": "The population in Zone 5 of Afar region (around 205,000 individuals) are aware of the harmful consequences of female genital mutilation.",
                       "assumptions": "The awareness of the population concerns all the types of female genital mutilation, including the least severe forms.",
                       "activities": [
                           {
                               "description": "to conduct KAP surveys",
                               "prerequesites": ""
                           },
                           {
                               "description": "to develop information, education, communication for the population on female genital mutilation",
                               "prerequesites": ""
                           },
                           {
                               "description": "to train and support114 Health Extension Workers (HEW), 92 skilled health workers, 548 teachers on IEC on female genital mutilation",
                               "prerequesites": ""
                           }
                       ],
                       "indicators": [
                           "d4f4e2a1-141c-59d4-9027-3cfbf9ca17ed"
                       ]
                   },
                   {
                       "description": "Prevention and management of complications and consequences of female genital mutilation is integrated in the continuum of care provided in 57 health facilities in Zone 5 of Afar region.",
                       "assumptions": "Health facilities are functional and there is no disruption in the continuum of care.",
                       "activities": [
                           {
                               "description": "to train and support 92 skilled health workers on female genital mutilation in the continuum of care",
                               "prerequesites": ""
                           },
                           {
                               "description": "to introduce materials and protocols in 57 health facilities for the integration of WHO recommendations related to female genital mutilation",
                               "prerequesites": ""
                           },
                           {
                               "description": "to set up protocols (including transfers to Dalifage hospital) for the opening of type III FGM during the second quarter of pregnancy",
                               "prerequesites": ""
                           }
                       ],
                       "indicators": [
                           "7003ff8b-4335-4682-9c48-82eb2320dfd2"
                       ]
                   },
                   {
                       "description": "Female genital mutilation is a focus of community dialogue in 57 kebele in Zone 5 of Afar region and communities choose to abandon the practice.",
                       "assumptions": "Community leaders (clan leaders, religious leaders...) and traditional authorities (sultan) are influential and instrumental enough to convince their communities to abandon the practice of female genital mutilation.",
                       "activities": [
                           {
                               "description": "to train the local partner ACISDA on community dialogue facilitation methodology",
                               "prerequesites": ""
                           },
                           {
                               "description": "to set up, train and support 57 local anti-FGM committees",
                               "prerequesites": ""
                           },
                           {
                               "description": "to facilitate community dialogue sessions on prevention of female genital mutilation in 57 kebele",
                               "prerequesites": ""
                           }
                       ],
                       "indicators": [
                       ]
                   },
                   {
                       "description": "Knowledge, and prevention, of female genital mutilation is introduced and supported at regional level.",
                       "assumptions": "All the stakeholders involved in prevention of female genital mutilation in Afar region are known. All of them agree to share information and their experiences.",
                       "activities": [
                           {
                               "description": "to prepare and write memoranda of understanding between partners and other stakeholders",
                               "prerequesites": ""
                           },
                           {
                               "description": "to facilitate regional networking between partners and stakeholders involved in prevention of female genital mutilation",
                               "prerequesites": ""
                           }
                       ],
                       "indicators": [
                       ]
                   }
